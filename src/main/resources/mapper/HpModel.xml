<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.npc.plantSCM.dao.HpModelDao">

    <insert id="insertModel" parameterType="String">
        insert into hp_model(model) value(#{model})
    </insert>

    <delete id="deleteModel" parameterType="String">
        delete from hp_model where model = #{model}
    </delete>
</mapper>